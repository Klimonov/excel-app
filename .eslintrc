{
  "parser": "babel-eslint",
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "extends": ["eslint:recommended", "google"],
  "rules": {
    "semi": "off",
    "comma-dangle": "off",
    "arrow-parens": "off",
    "linebreak-style": 0,
    "global-require": 0,
    "require-jsdoc": "off",
    "eslint linebreak-style": [
      0,
      "error",
      "windows"
    ]
  }
}